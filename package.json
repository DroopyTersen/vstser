{
  "name": "vstser",
  "version": "1.0.0",
  "description": "PWA for browsing VSTS Projects",
  "main": "index.html",
  "scripts": {
    "prebuild": "npm run clean",
    "build": "webpack",
    "postbuild": "node tasks/build.js",
    "serve": "webpack-dev-server --open --port 1234",
    "start": "npm-run-all --parallel serve watch:public",
    "watch": "npm-run-all --parallel watch:src watch:public",
    "watch:src": "webpack --watch",
    "watch:public": "node tasks/watch.js",
    "clean": "rimraf dist"
  },
  "repository": {
    "type": "git",
    "url": "https://andrewpetersen.visualstudio.com/DefaultCollection/_git/mohanna"
  },
  "author": "Andrew Petersen",
  "license": "MIT",
  "devDependencies": {
    "css-loader": "^0.28.11",
    "file-loader": "^1.1.11",
    "fs-extra": "^6.0.1",
    "node-sass": "^4.9.0",
    "npm-run-all": "^4.1.3",
    "parcel-plugin-bundle-visualiser": "^1.1.2",
    "rimraf": "^2.6.2",
    "sass-loader": "^7.0.3",
    "style-loader": "^0.21.0",
    "ts-loader": "^4.4.1",
    "typescript": "^2.9.1",
    "webpack": "^4.12.0",
    "webpack-bundle-analyzer": "^2.13.1",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.1.4"
  },
  "dependencies": {
    "@fortawesome/fontawesome": "^1.1.8",
    "@fortawesome/fontawesome-free-regular": "^5.0.13",
    "@fortawesome/fontawesome-free-solid": "^5.0.13",
    "@fortawesome/react-fontawesome": "^0.0.20",
    "@reach/router": "^1.0.2",
    "@types/node": "^9.6.6",
    "@types/react": "^16.3.16",
    "@types/react-dom": "^16.0.5",
    "date-fns": "^1.29.0",
    "hub-flow": "^0.0.6",
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "react-pose": "^2.1.2"
  }
}
